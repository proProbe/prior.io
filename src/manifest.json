{
  "name": "Prior.IO",
  "description": "This extension alerts whenever a page has changed",
  "manifest_version": 2,
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://*/*",
    "https://*/*",
    "http://www.google.com/"
  ]
}
